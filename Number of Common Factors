class Solution {
    public int commonFactors(int a, int b) {
        int l=0;
        if(a<b){
            l=a;
        }else{
            l=b;
        }
        int count=0;
        for(int i=1;i<=l;i++){
            if((a%i==0)&&(b%i==0)){
                count++;
            }
        }
        return count;
    }
}
